 243              	/* Reset Handler */
 244              	    .thumb_func
 245              	    .globl  Reset_Handler
 247              	Reset_Handler:
 248              	    ldr sp,=__initial_sp
 249              	
 250              	#ifndef __SKIP_SYSTEM_INIT
 251              	    ldr  r0, =SystemInit
 252              	    blx  r0
 253              	#endif
 254              	
 255              	/* Initialize data
 256              	 *
 257              	 *  Between symbol address __copy_table_start__ and __copy_table_end__,
 258              	 *  there are array of triplets, each of which specify:
 259              	 *    offset 0: LMA of start of a section to copy from
 260              	 *    offset 4: VMA of start of a section to copy to
 261              	 *    offset 8: size of the section to copy. Must be multiply of 4
 262              	 *
 263              	 *  All addresses must be aligned to 4 bytes boundary.
 264              	 */
 265 0208 234C     		ldr	r4, =__copy_table_start__
 266 020a 244D     		ldr	r5, =__copy_table_end__
 267              	
 268              	.L_loop0:
 269 020c AC42     		cmp	r4, r5
 270 020e 09DA     		bge	.L_loop0_done
 271 0210 2168     		ldr	r1, [r4]
 272 0212 6268     		ldr	r2, [r4, #4]
 273 0214 A368     		ldr	r3, [r4, #8]
 274              	
 275              	.L_loop0_0:
 276 0216 043B     		subs	r3, #4
 277 0218 A2BF     		ittt	ge
 278 021a C858     		ldrge	r0, [r1, r3]
 279 021c D050     		strge	r0, [r2, r3]
 280 021e FAE7     		bge	.L_loop0_0
 281              	
 282 0220 0C34     		adds	r4, #12
 283 0222 F3E7     		b	.L_loop0
 284              	
 285              	.L_loop0_done:
 286              	
 287              	/* Zero initialized data 
 288              	 *  Between symbol address __zero_table_start__ and __zero_table_end__,
 289              	 *  there are array of tuples specifying:
 290              	 *    offset 0: Start of a BSS section
 291              	 *    offset 4: Size of this BSS section. Must be multiply of 4
 292              	 *
 293              	 *  Define __SKIP_BSS_CLEAR to disable zeroing uninitialzed data in startup.
 294              	 */    
 295              	#ifndef __SKIP_BSS_CLEAR
 296 0224 1E4B     		ldr	r3, =__zero_table_start__
 297 0226 1F4C     		ldr	r4, =__zero_table_end__
 298              	
 299              	.L_loop2:
 300 0228 A342     		cmp	r3, r4
 301 022a 08DA     		bge	.L_loop2_done
 302 022c 1968     		ldr	r1, [r3]
 303 022e 5A68     		ldr	r2, [r3, #4]
 304 0230 0020     		movs	r0, 0
 305              	
 306              	.L_loop2_0:
 307 0232 043A     		subs	r2, #4
 308 0234 A4BF     		itt	ge
 309 0236 8850     		strge	r0, [r1, r2]
 310 0238 FBE7     		bge	.L_loop2_0
 311              	
 312 023a 0833     		adds	r3, #8
 313 023c F4E7     		b	.L_loop2
 314              	.L_loop2_done:    
 315              	#endif /* __SKIP_BSS_CLEAR */
 316              	   
 317              	#ifndef __SKIP_LIBC_INIT_ARRAY
 318 023e 1A48     	    ldr  r0, =__libc_init_array
 319 0240 8047     	    blx  r0
 320              	#endif
 321              	
 322 0242 1A48     	    ldr  r0, =main
 323 0244 8047     	    blx  r0
