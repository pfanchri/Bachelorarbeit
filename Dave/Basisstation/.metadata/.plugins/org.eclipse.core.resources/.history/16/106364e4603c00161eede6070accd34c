  65              	/* ===========START : MACRO DEFINITION MACRO DEFINITION ================== */
  66              	 
  67              	.macro Entry Handler
  68              	#if defined(ENABLE_CPU_CM_001_WORKAROUND)
  69              	    .long \Handler\()_Veneer
  70              	#else
  71              	    .long \Handler
  72              	#endif
  73              	.endm
  74              	
  75              	.macro Insert_ExceptionHandler Handler_Func 
  76              	    .weak \Handler_Func
  77              	    .thumb_set \Handler_Func, Default_Handler
  78              	
  79              	#if defined(ENABLE_CPU_CM_001_WORKAROUND)
  80              	    .weak \Handler_Func\()_Veneer
  82              	\Handler_Func\()_Veneer:
  83              	    push {r0, lr}
  84              	    ldr  r0, =\Handler_Func
  85              	    blx  r0
  86              	    pop  {r0, pc}
  88              	#endif 
  89              	.endm
  90              	
  91              	/* =============END : MACRO DEFINITION MACRO DEFINITION ================== */
