		<p1:GTab enabled="true" text="Clock Generation Settings" widgetName="gtab_clockgeneration">
			<p1:GTab.control>
				<p1:Tab>
					<p1:Tab.layout>
						<GridLayout/>
					</p1:Tab.layout>
					<p1:GGroup text="Main PLL Settings" widgetName="ggroup_mainpllset">
						<p1:GGroup.layoutData>
							<GridData widthHint="716"/>
						</p1:GGroup.layoutData>
						<p1:GGroup.layout>
							<GridLayout numColumns="4"/>
						</p1:GGroup.layout>
						<p1:GLabel text="PLL clock source:" widgetName="glabel_pllclksrc">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="LEFT"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GCombo description="The Main PLL converts a low-frequency external clock signal into a high-speed internal&#13;&#10;clock. And the Main PLL clock is a main source of System frequency (fSYS) and it is derived from following sources:&#13;&#10;&lt;OL&gt;&#13;&#10;&lt;LI&gt;Internal Fast Oscillator: Slow internal 24MHz clock. Used during normal or low power operations after the startup.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;External Crystal High Precision Oscillator: Used to generate a high precision clock via XTAL1 and XTAL2 pins. Alternatively, an external clock is being supplied to Clock generation unit (CGU) from CLKIN pin, via the OSC_HP module, bypassing the oscillator circuit.&lt;/LI&gt;&lt;/OL&gt;" manifestObj="true" options="Internal Fast Oscillator&#13;&#10;External Crystal High Precision Oscillator" toolTipText="Select a source of Main PLL clock" value="External Crystal High Precision Oscillator" widgetName="gcombo_pllclksrc">
							<p1:GCombo.layoutData>
								<GridData horizontalAlignment="LEFT" horizontalSpan="3" widthHint="224"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
						<p1:GLabel text="PLL operating mode:" widgetName="glabel_mainpllset">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="LEFT"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GCombo description="The Operating Mode selection decides to bypass a VCO or not:&#13;&#10;&lt;OL&gt;&lt;LI&gt; Normal Mode: fPLL is a scaled version of VCO Clock&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;  &lt;B&gt;fPLL = (N * fSOC) / (P * K2)&lt;/LI&gt;&lt;/B&gt;&#13;&#10;&lt;LI&gt;Prescaler Mode: fPLL is running at external frequency fOSC (bypassing a VCO)&lt;BR&gt;&#13;&#10;&#9;&#9;&#9;&#9; &lt;B&gt;fPLL = fOSC / K1&lt;/B&gt;&lt;/LI&gt;&lt;/OL&gt;" manifestObj="true" options="Normal Mode&#13;&#10;Prescaler Mode" toolTipText="Select a desired Mode to bypass a VCO or not" value="Normal Mode" widgetName="gcombo_mainpllset">
							<p1:GCombo.layoutData>
								<GridData horizontalAlignment="LEFT" horizontalSpan="3" widthHint="224"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
						<p1:GLabel manifestObj="true" text="Requested PLL frequency [MHz]:" widgetName="glabel_mainplldesfreq">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="LEFT" widthHint="177"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GFloat format="*,6" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="120.0" minValue="3.0" value="120" widgetName="gfloat_mainplldesfreq" x:Style="BORDER" description="The Requested PLL frequency internally configures the PLL dividers: &#13;&#10;&lt;B&gt;NDIV, PDIV, K2DIV and K1DIV:&lt;/B&gt;&#13;&#10;&lt;OL&gt;&lt;LI&gt;In Normal Mode: fPLL is a scaled version of VCO Clock&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fPLL = (N * fSOC) / (P * K2) &lt;/B&gt;&lt;/LI&gt;&#13;&#10;&lt;LI&gt;In Prescaler Mode: fPLL is running at external frequency fOSC (bypassing a VCO)&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fPLL = fOSC / K1&lt;/B&gt;&lt;/LI&gt;&lt;/OL&gt;" toolTipText="The Requested PLL frequency internally configures the PLL dividers: NDIV, PDIV, K2DIV and K1DIV.">
							<p1:GFloat.layoutData>
								<GridData horizontalAlignment="LEFT" widthHint="50"/>
							</p1:GFloat.layoutData>
						</p1:GFloat>
						<p1:GLabel manifestObj="true" text="Actual PLL frequency [MHz]:" widgetName="glabel_mainpllactfreq">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="RIGHT"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GFloat enabled="false" format="*,6" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="120.0" minValue="3.0" value="120" widgetName="gfloat_mainpllactfreq" x:Style="BORDER" description="The Actual PLL frequency internally configures the PLL dividers: &#13;&#10;&lt;B&gt;NDIV, PDIV, K2DIV and K1DIV:&lt;/B&gt;&#13;&#10;&lt;OL&gt;&lt;LI&gt;In Normal Mode: fPLL is a scaled version of VCO Clock&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fPLL = (N * fSOC) / (P * K2) &lt;/B&gt;&lt;/LI&gt;&#13;&#10;&lt;LI&gt;In Prescaler Mode: fPLL is running at external frequency fOSC (bypassing a VCO)&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fPLL = fOSC / K1&lt;/B&gt;&lt;/LI&gt;&lt;/OL&gt;">
							<p1:GFloat.layoutData>
								<GridData grabExcessHorizontalSpace="true" widthHint="46"/>
							</p1:GFloat.layoutData>
						</p1:GFloat>
					</p1:GGroup>
					<p1:GGroup text="High Precision Oscillator Settings" widgetName="ggroup_extclk" manifestObj="true">
						<p1:GGroup.layoutData>
							<GridData horizontalAlignment="FILL"/>
						</p1:GGroup.layoutData>
						<p1:GGroup.layout>
							<GridLayout numColumns="2"/>
						</p1:GGroup.layout>
						<p1:GLabel manifestObj="true" text="Operating mode:" widgetName="glabel_extclkmode">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="LEFT"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GCombo mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="External Crystal Mode&#13;&#10;External Clock Input Mode" toolTipText="Select a source of external input to the PLL" value="External Crystal Mode" widgetName="gcombo_extclkmode" description="High precision oscillator derived from the following sources:&#13;&#10;&lt;OL&gt;&#13;&#10;&lt;LI&gt;External Crystal Mode: Used to generate a high precision clock via XTAL1 and XTAL2 pins.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;External Clock Input Mode: An external clock is being supplied to Clock generation unit (CGU) from CLKIN pin, via the OSC_HP module, bypassing the oscillator circuit.&lt;/LI&gt;&#13;&#10;&lt;/OL&gt;">
							<p1:GCombo.layoutData>
								<GridData grabExcessHorizontalSpace="true" horizontalAlignment="LEFT" widthHint="224"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
						<p1:GLabel text="External clock frequency [MHz]:" widgetName="glabel_extclkoscfreq" manifestObj="true">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="RIGHT" widthHint="176"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GFloat description="The External Clock using Crystal Oscillator or CLKIN pin is a source of Main-PLL &amp; USBPLL clocks." format="*,2" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="25.0" minValue="4.0" toolTipText="Desired External Clock Frequency [in MHz]" value="12" widgetName="gfloat_extclkoscfreq" x:Style="BORDER">
							<p1:GFloat.layoutData>
								<GridData grabExcessHorizontalSpace="true" horizontalAlignment="LEFT" widthHint="50"/>
							</p1:GFloat.layoutData>
						</p1:GFloat>
					</p1:GGroup>
					<p1:GGroup text="Internal Fast Oscillator Settings" widgetName="ggroup_fofi">
						<p1:GGroup.layoutData>
							<GridData horizontalAlignment="FILL"/>
						</p1:GGroup.layoutData>
						<p1:GGroup.layout>
							<GridLayout numColumns="2"/>
						</p1:GGroup.layout>
						<p1:GLabel text="Calibration:" widgetName="glabel_backupclktrim">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="RIGHT" widthHint="178"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GCombo description="The internal fast oscillator options are used to calibrate the Backup Clock by improving the accuracy. Following options are available:&#13;&#10;&lt;OL&gt;&lt;LI&gt;Factory Calibration: The factory calibration enabled with user software via bit field FOTR of the PLLCON0 register.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Automatic Calibration Using Standby Clock: Backup Clock adjustment based on the  high precision reference clock fSTDBY. The automatic calibration enabled with user software via bit field AOTREN of the PLLCON0 register.&lt;/LI&gt;&#13;&#10;&lt;/OL&gt;" manifestObj="true" options="Factory Calibration&#13;&#10;Automatic Calibration Using Standby Clock" toolTipText="Select a BackUp Clock Trimming Option&#13;&#10;" value="Factory Calibration" widgetName="gcombo_backupclktrim">
							<p1:GCombo.layoutData>
								<GridData grabExcessHorizontalSpace="true" horizontalAlignment="LEFT" widthHint="224"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
					</p1:GGroup>
				</p1:Tab>
			</p1:GTab.control>
		</p1:GTab>
		<p1:GTab text="Standby Clock Generation Settings" visible="true" enabled="true" widgetName="gtab_standbyclkgeneration">
			<p1:GTab.control>
				<p1:Tab widgetName="composite11">
					<p1:Tab.layout>
						<GridLayout/>
					</p1:Tab.layout>
					<p1:GGroup manifestObj="true" text="Standby Clock (fSTDBY)" widgetName="ggroup_standbyclkfreq">
						<p1:GGroup.layoutData>
							<GridData widthHint="716" horizontalAlignment="FILL"/>
						</p1:GGroup.layoutData>
						<p1:GGroup.layout>
							<GridLayout numColumns="2"/>
						</p1:GGroup.layout>
						<p1:GLabel text="Clock source:" widgetName="glabel_standbyclksrc">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="RIGHT"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GCombo description="The standby clock of 32.768kHz is generated in hibernate domain:&#13;&#10;&lt;OL&gt;&lt;LI&gt;External Crystal Ultra Low Power Oscillator: Precise and stable clock is propagated to Core domain as fSTDBY and used for continuos adjustments the fast internal backup clock source fOFI.&lt;/LI&gt;&#13;&#10;&lt;LI&gt; Internal Slow Oscillator: Used for independent clock source for WDT module and as the clock for periodic wakeup events in power saving modes, and for continuos adjustments the fast internal backup clock source fOFI.&lt;/LI&gt;&#13;&#10;&lt;/OL&gt;" manifestObj="true" options="External Crystal Ultra Low Power Oscillator&#13;&#10;Internal Slow Oscillator" toolTipText="Select a source of Standby Clock" value="Internal Slow Oscillator" widgetName="gcombo_standbyclksrc">
							<p1:GCombo.layoutData>
								<GridData grabExcessHorizontalSpace="true" widthHint="228"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
					</p1:GGroup>
					<p1:GGroup manifestObj="true" text="RTC Clock (fRTC)" widgetName="ggroup_rtcclkfreq">
						<p1:GGroup.layoutData>
							<GridData widthHint="299" horizontalAlignment="FILL"/>
						</p1:GGroup.layoutData>
						<p1:GGroup.layout>
							<GridLayout numColumns="2"/>
						</p1:GGroup.layout>
						<p1:GLabel text="Clock source:" widgetName="glabel_rtcclksrc">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="RIGHT"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GCombo description="The real-time clock 32.768kHz frequency (fRTC) is derived from:&#13;&#10;&lt;OL&gt;&#13;&#10;&lt;LI&gt;External Crystal Ultra Low Power Oscillator: Precise and stable clock is propagated to Core domain.&lt;/LI&gt;&#13;&#10;&lt;LI&gt; Internal Slow Oscillator: Used for independent clock source. When the external crystal failure the clock source switches automatically to the Internal Slow Clock Source.&lt;/LI&gt;&#13;&#10;&lt;/OL&gt;" manifestObj="true" options="External Crystal Ultra Low Power Oscillator&#13;&#10;Internal Slow Oscillator" toolTipText="Select a source of Standby Clock" value="Internal Slow Oscillator" widgetName="gcombo_rtcclksrc">
							<p1:GCombo.layoutData>
								<GridData grabExcessHorizontalSpace="true" widthHint="228"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
					</p1:GGroup>
				</p1:Tab>
			</p1:GTab.control>
		</p1:GTab>
		<p1:GTab text="Clock Selection Settings" visible="true" enabled="true" widgetName="gtab_clockselection">
			<p1:GTab.control>
				<p1:Tab widgetName="composite_8" layout="{x:Null}">
					<p1:GGroup text="System Clock (fSYS)" widgetName="ggroup_sysclkfreq" bounds="5, 5, 721, 81">
						<p1:GGroup.layout>
							<GridLayout numColumns="4"/>
						</p1:GGroup.layout>
						<p1:GLabel text="Clock source:" widgetName="glabel_sysclksrc"/>
						<p1:GCombo description="The System Frequency (fSYS) is used for CPU and all peripherals, and derived from:&#13;&#10;&lt;OL&gt;&#13;&#10;&lt;LI&gt;Main PLL Clock: Converts a low-frequency external clock signal to a high-speed internal clock. Triggers autonomously emergency action if it loses its lock on the external clock and switches to the Fast Internal Backup Clock.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Internal Fast Oscillator: Generated internally, it is a default clock after start-up. Used for bypassing the PLL in startup. Furthermore used as independent clock source for the watchdog module and system clock during normal operation. When prescaler mode the clock used as emergency clock if the external clock failure is detected.&lt;/LI&gt;&#13;&#10;&lt;/OL&gt;" manifestObj="true" options="Main PLL Clock&#13;&#10;Internal Fast Oscillator" toolTipText="Select a source of system frequency" value="Main PLL Clock" widgetName="gcombo_sysclksrc">
							<p1:GCombo.layoutData>
								<GridData horizontalSpan="2" widthHint="146"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
						<Label/>
						<p1:GLabel text="Clock divider:" widgetName="glabel_sysclkdiv">
							<p1:GLabel.layoutData>
								<GridData widthHint="120"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GInteger format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="256" minValue="1" value="1" widgetName="gint_sysclkdiv" toolTipText="Scale downs the System clock" description="Scale downs the System clock">
							<p1:GInteger.layoutData>
								<GridData widthHint="26"/>
							</p1:GInteger.layoutData>
